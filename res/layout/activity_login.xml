<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0sp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="69sp"
        android:background="#FFDB6D"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <org.tensorflow.demo.font.OutlineTextView

            android:layout_width="match_parent"
            android:layout_height="54sp"
            android:fontFamily="@font/gmarket"
            android:gravity="center"
            android:shadowColor="#000000"
            android:shadowDx="2.0"
            android:shadowDy="2.0"
            android:shadowRadius="1.5"
            android:text="Navi"
            android:textColor="#ffffff"
            android:textSize="25sp"
            app:textStroke="true"
            app:textStrokeColor="@android:color/black"
            app:textStrokeWidth="5.0">

        </org.tensorflow.demo.font.OutlineTextView>

    </LinearLayout>

    <Button
        android:id="@+id/login_gotoJoin"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="54dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="103dp"
        android:background="@drawable/button_style2"
        android:fontFamily="@font/gmarket"
        android:gravity="center"
        android:text="회원가입"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_gotoLogin" />

    <Button
        android:id="@+id/bt"
        android:layout_width="164dp"
        android:layout_height="43dp"
        android:background="@drawable/button_style2"
        android:fontFamily="@font/gmarket"
        android:gravity="center"
        android:text="블루투스"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_gotoJoin" />

    <EditText
        android:id="@+id/login_password"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="54dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="26dp"
        android:background="@drawable/edittext_style2"
        android:fontFamily="@font/gmarket"
        android:gravity="center"
        android:hint="  비밀번호를 입력하세요"
        android:inputType="numberPassword"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/login_gotoLogin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_id">

    </EditText>

    <androidx.appcompat.widget.AppCompatButton

        android:id="@+id/login_gotoLogin"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="54dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="23dp"
        android:background="@drawable/button_style2"
        android:fontFamily="@font/gmarket"
        android:gravity="center"
        android:text="로그인"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/login_gotoJoin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_password" />

    <EditText

        android:id="@+id/login_id"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="54dp"
        android:layout_marginEnd="54dp"
        android:layout_marginBottom="17dp"
        android:background="@drawable/edittext_style2"
        android:fontFamily="@font/gmarket"
        android:gravity="center"
        android:hint="아이디를 입력하세요"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/login_password"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

    </EditText>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="23dp"
        android:fontFamily="@font/gmarket"
        android:text="아이디와 비밀번호를 입력해주세요"
        android:textSize="17sp"
        app:layout_constraintBottom_toTopOf="@+id/login_id"
        app:layout_constraintStart_toStartOf="@+id/login_id"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <org.tensorflow.demo.font.OutlineTextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="134dp"
        android:fontFamily="@font/gmarket"
        android:shadowColor="#000000"
        android:shadowDx="2.0"
        android:shadowDy="2.0"
        android:shadowRadius="1.5"
        android:text="로그인"
        android:textColor="#FAD251"
        android:textSize="50sp"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        app:textStroke="true"
        app:textStrokeColor="@android:color/black"
        app:textStrokeWidth="5.0" />


</androidx.constraintlayout.widget.ConstraintLayout>